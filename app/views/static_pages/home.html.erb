<% if signed_in? %>
  <div class="row">
    <aside class="span4">
      <section>
        <%= render 'shared/user_info' %>
      </section>
      <section>
        <%= render 'shared/stats' %>
      </section>
    </aside>
    <div class="span8">
      <section>
        <%= render 'shared/micropost_form' %>
      </section>
      <h3>Micropost Feed</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <div class = "complete-unit">
    <div class ="welcome-unit">
      <h1>Welcome to SF DATE</h1>
      <h2>
        This is the home page of SF DATE, a dating site for SFSU students
      </h2>
    </div>
    <div class="center hero-unitc">
      <h1>Sign Up Now!</h1>
  
      <%= link_to "Register", signup_path, class: "btn btn-large btn-primary" %>
    </div>
  </div>

  <!-- image that links to a page -->
  <!-- first # is path to image name located in app/assets/images, and the second # is the url of the link -->
  <div class="logo"><%= link_to image_tag("SFDate.png", alt: "SFDate"), '#' %></div>
<% end %>