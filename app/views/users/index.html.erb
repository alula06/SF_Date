<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= form_tag(users_path, method: "get") do %>
  <%= label_tag(:search, "Search for:") %>
  <%= text_field_tag(:search, params[:search], 'placeholder' => 'User Search') %>
  <%= submit_tag("Search", :name =>  nil) %>
<% end %>

<%#= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%#= //will_paginate %>